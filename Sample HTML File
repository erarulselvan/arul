<script type="text/javascript">
if($.cookie("css") || $.cookie("img")) {
	$("link.changeme").attr("href",$.cookie("css"));
	$("img.dirname").attr("href",$.cookie("img"));
}
$(document).ready(function() { 
	$("#navigation123 li a").click(function() { 	
		$("link.changeme").attr("href",$(this).attr('rel'));
		$("img.dirname").attr("href",$(this).attr('dir'));
		$.cookie("css",$(this).attr('rel'), {expires: 365, path: '/'});
		$.cookie("img",$(this).attr('dir'), {expires: 365, path: '/'});
		return false;
	});

	  // Initialise the tag canvas
  TagCanvas.textFont = 'Arial, Helvetica, sans-serif';
	TagCanvas.textColour = '#FFF';
	TagCanvas.textHeight = 50;
	TagCanvas.outlineColour = '#FFF';
	TagCanvas.maxSpeed = 0.03;
	TagCanvas.minBrightness = 0.2;
	TagCanvas.depth = 0.45;
	TagCanvas.pulsateTo = 0.6;
	TagCanvas.initial = [0.1,-0.1];
	TagCanvas.decel = 0.98;
	TagCanvas.reverse = true;
	TagCanvas.hideTags = false;
	TagCanvas.weight = true;
	TagCanvas.weightMode = 'size';

	try {
    var canstart = false;
    if (jQuery.browser.msie) {
      if (parseInt(jQuery.browser.version) > 8) {
        canstart = true;
      }
    }
    else {
      canstart = true;
    }
    if (canstart) {
      TagCanvas.Start('tagcanvas', 'tagcontain');
    }
	}
	catch(e) {
    if (document.getElementById('cmsg') != null) {
      document.getElementById('cmsg').style.display='none';
      document.getElementsByTagName('canvas')[0].style.border='0';
    }
	}	
	
	
	
	
});
</script>

